# top most editorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

#set default charset
[*.js]
charset = utf-8

# space indentation
[*.js]
indent_style = space
indent_size = 2

# Matches the exact files either package.json
indent_style = space
indent_size = 2

[*.md]
trim_trailing_whitespace = false
